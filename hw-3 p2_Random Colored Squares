void setup() {
  size (500, 500); //canvas size
  background (255);
  noStroke();
  colorMode (HSB, 100);
}
void draw() {
  int x=10;
  while (x<480) {
    int y=20;
    while (y<480) {
      fill (random(100), random(100), (100));
      rect (x, y, 10, 10); //x,y,width,hieght
      y=y+20;
    }
    x=x+20;
  }
  noLoop();
}

//problem 2
