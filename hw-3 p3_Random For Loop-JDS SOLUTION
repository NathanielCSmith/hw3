int hi = 10;
int border =50;

void setup() {
  size(600, 600);
  background(255);
  stroke(0);

  rectMode(CENTER);
  //change the x coordinate
  for (int i=0; i<25; i++) { //for (start, stop; step)
    int x = border + (i*20);
    //change the y coordinate
    for (int j=0; j<25; j++) {
      int y = border + (j*20);
      if (random(7) > 2) {
        rect(x, y, hi, hi); //border + x coord, y coord, width, hieght
        //for (int
      }
    }
  }
}

